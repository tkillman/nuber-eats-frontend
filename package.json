    "@fortawesome/react-fontawesome": "^0.2.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^14.0.0",
    "graphql": "^16.10.0",
    "mock-apollo-client": "^1.3.1",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "5.3.4",
    "react-scripts": "5.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.4.2",
    "victory": "^37.3.6",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "tailwind:build": "tailwindcss build src/styles/tailwind.css -o src/styles/styles.css",
    "generate": "rimraf src/__generated__ && graphql-codegen",
    "start": "yarn run generate & yarn run tailwind:build & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose",
    "eject": "react-scripts eject",
    "test:cover": "yarn test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@testing-library/cypress": "^10.0.3",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "cypress-mochawesome-reporter": "^3.8.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/components/*.tsx",
      "./src/pages/*.tsx",
      "./src/routes/*.tsx"
    ]
  }
}
